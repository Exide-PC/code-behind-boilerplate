{
    "compilerOptions": {
        "baseUrl": "./src",
        "target": "es5",
        "jsx": "react",
        "sourceMap": true,
        
        "strict": true,
		"allowUnreachableCode": false,
		"noImplicitAny": true,
		"noImplicitThis": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"forceConsistentCasingInFileNames": true,
    }
}
