{
    "compilerOptions": {
		"baseUrl": "./src",
        "target": "es5",
        "jsx": "react",
        "sourceMap": true,
        
        "strict": true,
		"allowUnreachableCode": false,
		"noImplicitAny": false,
		"noImplicitThis": true,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"forceConsistentCasingInFileNames": true,
	},
	"include": ["src/**/*"]
}
